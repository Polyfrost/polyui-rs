{
  "package": {
    "productName": "OneLauncher",
    "version": "0.1.0"
  },
  "build": {
    "devPath": "http://localhost:8001/",
    "distDir": "../dist",
    "beforeDevCommand": "pnpm exec vite --clearScreen=false",
    "beforeBuildCommand": "pnpm exec vite build"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "cc.polyfrost.onelauncher",
      "icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"resources": [],
			"externalBin": [],
			"copyright": "Polyfrost",
			"shortDescription": "Next generation Minecraft launcher",
			"deb": {
				"depends": []
			},
			"macOS": {
				"frameworks": [],
				"minimumSystemVersion": "10.15",
				"exceptionDomain": "",
				"signingIdentity": null,
				"entitlements": null
			},
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": true,
      "os": {
        "all": true
      },
      "dialog": {
        "all": true
      }
    },
    "windows": [
      {
        "title": "OneLauncher",
        "width": 1400,
				"height": 725,
				"minWidth": 700,
				"minHeight": 500,
				"resizable": true,
				"fullscreen": false,
				"alwaysOnTop": false,
				"focus": false,
				"visible": false,
				"fileDropEnabled": false,
				"decorations": true,
				"transparent": true,
				"center": true
      }
    ],
    "security": {
      "csp": "default-src onelauncher: asset: https://asset.localhost blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}